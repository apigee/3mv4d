<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="OpenWeather">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AssignOpenweatherRequestParam</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>http://api.openweathermap.org/data/2.5/weather</URL>
    </HTTPTargetConnection>
</TargetEndpoint>