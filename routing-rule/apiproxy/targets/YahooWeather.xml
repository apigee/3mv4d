<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="YahooWeather">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AssignYahooweatherRequestParam</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response>
            <Step>
                <Name>ConvertResponsetoJson</Name>
            </Step>
        </Response>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>http://weather.yahooapis.com/forecastrss</URL>
    </HTTPTargetConnection>
</TargetEndpoint>